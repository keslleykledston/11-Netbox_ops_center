MOVIDESK_API_URL=https://api.movidesk.com/public/v1
MOVIDESK_TOKEN=64e0619b-43a0-44cd-8d3d-61ce73be5061
MOVIDESK_SYNC_INTERVAL=600
MOVIDESK_SYNC_ENABLED=true
