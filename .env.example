VITE_USE_BACKEND=true
VITE_API_URL=/api
VITE_HUB_API_URL=/hub-api
API_SERVER_URL=http://localhost:4000
SNMP_SERVER_URL=http://localhost:3001

# Database (Postgres)
DATABASE_URL=postgresql://netbox_ops:netbox_ops@db:5432/netbox_ops

# SNMP gateway
SNMP_SERVER_PORT=3001
SNMP_MAX_REPETITIONS=20
SNMP_GLOBAL_TIMEOUT_MS=60000

# NetBox
NETBOX_URL=https://docs.k3gsolutions.com.br
NETBOX_TOKEN=b16412d4dc73d4d9f820e79461e13b3faa6953ac
NETBOX_TENANT_GROUP_FILTER=K3G Solutions

# Jumpserver
JUMPSERVER_URL=
JUMPSERVER_TOKEN=
JUMPSERVER_BATCH_SIZE=50
JUMPSERVER_FUZZY_THRESHOLD=0.7

# NetBox custom field to store JumpServer asset ID
NETBOX_JUMPSERVER_ID_FIELD=jumpserver_asset_id

# Oxidized integration
OXIDIZED_API_URL=http://localhost:8888
# Caminho do router.db (host)
OXIDIZED_ROUTER_DB=/etc/oxidized/router.db

# ASN Lookup Configuration
# Habilita/desabilita lookup automático de nomes de ASN
ASN_LOOKUP_ENABLED=true
# URL customizada para lookup de ASN (opcional). Use {asn} como placeholder
# Exemplo: ASN_LOOKUP_URL=https://api.custom.com/asn/{asn}
# Se não configurado, usa BGPView e RDAP APNIC como fallback
ASN_LOOKUP_URL=
